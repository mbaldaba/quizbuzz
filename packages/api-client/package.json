{
  "name": "@repo/api-client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "export-openapi": "tsx scripts/export-openapi.ts",
    "generate": "openapi-ts && node scripts/post-process.mjs",
    "codegen": "npm run export-openapi && npm run generate",
    "build": "tsc",
    "lint": "eslint .",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@repo/contracts": "*",
    "@tanstack/react-query": "^5.59.20",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "0.88.0",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "eslint": "^9.39.1",
    "tsx": "^4.19.2",
    "typescript": "~5.9.3"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "@tanstack/react-query": "^5.0.0"
  }
}
